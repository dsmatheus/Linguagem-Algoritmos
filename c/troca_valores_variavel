#include <stdio.h>
#include <stdlib.h>

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
	
	int vA, vB, troca;
	
	printf("Digite dois numeros inteiros: \n");
	scanf("%i %i", &vA, &vB);
	
	troca = vA;
	vA = vB;
	vB = troca;
	
	printf("\nVALORES TROCADOS = ");
	printf("\n %i", vA);
	printf("\n %i", vB);
	
	return 0;
}
